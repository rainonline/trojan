VERSION=v1.0.0
BUILD_DATE=2025-10-08
GIT_COMMIT=fcdc9b4

# ==========================================
# 数据库配置
# ==========================================
# 请务必修改为强密码！
MYSQL_ROOT_PASSWORD=your_secure_root_password_here_min_16_chars
MYSQL_PASSWORD=your_secure_trojan_password_here_min_16_chars

